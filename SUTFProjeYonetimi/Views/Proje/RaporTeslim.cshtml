@{
	ViewBag.Title = "Rapor Teslim";

	var proje = ViewData["Proje"] as SUTFProjeYonetimi.Models.Proje;
}


<div id="ribbon">
	<ol class="breadcrumb">
		<li>Akademisyen</li>
		<li>Akademisyen İşlemleri</li>
	</ol>
</div>
<div id="content">
	<article class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="margin-bottom:25px">
		<div class="jarviswidget jarviswidget-color-red" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">
			<header>
				<span class="widget-icon"> <i class="fa fa-edit"></i> </span>
				<h2>Akademisyen Formu</h2>
			</header>
			<div class="container-fluid">
				<div class="jarviswidget-editbox"></div>
				<div class="widget-body no-padding">
					@using (Html.BeginForm())
					{
						@Html.AntiForgeryToken()
						<div style="padding-bottom:10px"></div>
						<div class="form-horizontal col-lg-12 col-md-12 col-sm-12 col-xs-12">
							@Html.ValidationSummary(true, "", new { @class = "text-danger" })
							<div class="form-group">
								@Html.Label("Rapor Dosyası", htmlAttributes: new { @class = "control-label col-lg-2 col-md-2 col-sm-2 col-xs-2" })
								<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
									@Html.TextBox("rapor", "", new { @class = "form-control", type = "file" }) <br />
								</div>
							</div>
						</div>
					}
				</div>
			</div>
		</div>
	</article>
</div>